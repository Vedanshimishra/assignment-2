#include<iostream>
using namespace std;

int main(int argc, char** argv) {
    int n;

   
    cout << "Enter a number (1 to 10): ";
    cin >> n;

   
    if (n < 1 || n > 10) {
        cout << "Invalid input. Please enter a number between 1 and 10." << endl;
        return 1;
    }

   
    for (int i = 1; i <= 10; ++i) {
        cout << n << " * " << i << " = " << n * i << endl;
    }

    return 0; 
}
